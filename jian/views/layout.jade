doctype html
- var bootstrap = '3.3.7'
- var jquery = '3.2.1'
html
  head
    title= title
    link(rel='icon', href='/images/icon.png')
    link(rel='stylesheet', href='/libs/bootstrap-#{bootstrap}/css/bootstrap.min.css')
    link(rel='stylesheet', href='/libs/bootstrap-#{bootstrap}/css/bootstrap-theme.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    div.container
      block content
    script(src='/libs/jquery-#{jquery}.min.js')
    script(src='/libs/bootstrap-#{bootstrap}/js/bootstrap.min.js')
